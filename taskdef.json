{
    "family": "hello-world",
    "networkMode": "awsvpc",
    "containerDefinitions": [
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "firelens-container-hello-world",
                    "awslogs-region": "ap-southeast-2",
                    "awslogs-create-group": "true",
                    "awslogs-stream-prefix": "firelens"
                }
            },
            "memoryReservation": 50
        },
        {
            "name": "hello-world",
            "image": "{{image}}",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
        
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1vCPU",
    "memory": "2GB",
    "executionRoleArn": "arn:aws:iam::803282840586:role/ecs_task_execution_role"
}
